{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Zaqzaq",
  "version": "0.0.3",
  "identifier": "com.zaqzaq-palestinian-dictionary.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Zaqzaq",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self' 'unsafe-inline' 'unsafe-eval' tauri: asset: https: http: ws: wss: data: blob:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/arabic-dictionary.db"
    ]
  }
}